Cell["Define a function that generates Histograms", "Text",
 CellChangeTimes->{{3.687636033713108*^9, 3.687636040663886*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"histo", "[", 
   RowBox[{"list_", ",", "column_", ",", "title_", ",", 
    RowBox[{"imagesize_:", "Large"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "listM", "}"}], ",", "\[IndentingNewLine]", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"listM", "=", 
      RowBox[{"Sort", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"Transpose", "@", "list"}], ")"}], "[", 
         RowBox[{"[", "column", "]"}], "]"}], "[", 
        RowBox[{"[", 
         RowBox[{"2", ";;"}], "]"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"BarChart", "[", 
      RowBox[{
       RowBox[{"Counts", "@", "listM"}], ",", 
       RowBox[{"ChartLabels", "\[Rule]", "Automatic"}], ",", 
       RowBox[{"ImageSize", "\[Rule]", "imagesize"}], ",", 
       RowBox[{"Frame", "\[Rule]", "True"}], ",", 
       RowBox[{"PlotLabel", "\[Rule]", "title"}], ",", 
       RowBox[{"ChartStyle", "\[Rule]", "Gray"}], ",", 
       RowBox[{"ChartBaseStyle", "\[Rule]", 
        RowBox[{"EdgeForm", "[", "None", "]"}]}]}], "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.687636043312601*^9, 3.687636140111081*^9}, {
  3.68763632120459*^9, 3.6876363363887167`*^9}, {3.6876376734453373`*^9, 
  3.687637674258566*^9}}],

